<header>
  <h1>Active Users</h1>
</header>

<div class="actions">
  <%= link_to "Add User", new_user_path %>
</div>

<ul>
<% @users.each do |user| %>
<li>
  <%= link_to user.name, user %>
  [ <%= link_to "Edit", edit_user_path(user) %>
  | <%= link_to "Delete", user_path(user), method: :delete, confirm: "Delete '#{user.name}'?"  %>
  ]
</li>
<% end %>
</ul>
